{
  "extends": [
    "eslint:recommended",
    // "wesbos",
    "next/core-web-vitals"
  ],
  "env": {
    "es6": true,
    "jest/globals": true
  },
  "plugins": ["jest"],
  "rules": {
    "semi": ["error", "never"],
    "indent": ["error", 2],
    "jest/no-identical-title": "error",
    "quotes": ["error", "double"],

    "no-unused-vars": "error",
    "react/prop-types": 0,
    "react/jsx-props-no-spreading": 0,
    "import/no-unresolved": 0
  }
}
